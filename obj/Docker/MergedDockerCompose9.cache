services:
  vogcodechallenge.api:
    build:
      context: C:\Users\maenm\source\repos\maen_vog_backend_codechallenge
      dockerfile: VogCodeChallenge.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: VogCodeChallenge.API
      target: base
    container_name: VogCodeChallenge.API_4
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: vogcodechallengeapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/VogCodeChallenge.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\maenm\source\repos\maen_vog_backend_codechallenge\VogCodeChallenge.API:/app:rw
    - C:\Users\maenm\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\maenm\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\maenm\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\maenm\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\maenm\source\repos\maen_vog_backend_codechallenge:/src:rw
version: '3.4'